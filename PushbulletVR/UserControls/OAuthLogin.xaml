<UserControl x:Class="PushbulletVR.UserControls.OAuthLogin"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PushbulletVR"
            xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
            xmlns:metro="http://metro.mahapps.com/winfx/xaml/controls"
            xmlns:Gecko="clr-namespace:Gecko;assembly=Geckofx-Core"
            TextElement.Foreground="{DynamicResource MaterialDesignBody}"
            TextElement.FontWeight="Regular"
            TextElement.FontSize="13"
            TextOptions.TextFormattingMode="Ideal" 
            TextOptions.TextRenderingMode="Auto"
            BorderThickness="1"     
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <DockPanel>
        <WindowsFormsHost DockPanel.Dock="Top" Width="Auto" Height="Auto" Name="cntBox"/>
    </DockPanel>
</UserControl>
